{
  "name": "chew-away",
  "description": "chew away at your files",
  "version": "0.0.0",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/chew-away"
  },
  "bin": {
    "chew-away": "./cli.js"
  },
  "engines": {
    "node": "*"
  },
  "main": "chew-away.js",
  "files": [
    "lib/",
    "*.js"
  ],
  "dependencies": {
    "fs-extra": "^4.0.0",
    "glob": "^7.1.2",
    "imagemin": "^5.3.1",
    "jimp": "^0.2.28",
    "merge-options": "^1.0.0",
    "ora": "^1.3.0",
    "yaku": "^0.18.2"
  },
  "devDependencies": {
    "coffeescript": "^2.0.0-beta3",
    "imagemin-guetzli": "^1.0.0"
  },
  "keywords": [
    "imagemin",
    "manager"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "coffee --bare --no-header --compile --output lib/ src/*.coffee",
    "dev": "npm run build && ./cli.js",
    "test": "",
    "preversion": "npm run test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  }
}